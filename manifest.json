{
  "manifest_version": 3,
  "name": "Venue Calendar",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/calendar*"
      ],
      "js": [
        "misc/jquery.3.7.1.min.js",
        "src/observer.js",
        "src/language.js",
        "src/popup.js",
        "src/src.js"
      ],
      "css": [
        "style.css"
      ],
      "web_accessible_resources": [
        "text.json"
      ],
      "runAt": "document_idle"
    }
  ],
  "action": {
    "default_popup": "html/popup.html"
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://calendar.google.com/calendar*"
  ]
}
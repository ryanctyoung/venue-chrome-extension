{
  "manifest_version": 3,
  "name": "Venue Calendar",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/calendar*"
      ],
      "js": [
        "misc/jquery.3.7.1.min.js",
        "observer.js",
        "language.js",
        "src.js"
      ],
      "css": [
        "style.css"
      ],
      "web_accessible_resources": [
        "text.json"
      ],
      "runAt": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },

  "host_permissions": [
    "https://calendar.google.com/calendar*"
  ]
}